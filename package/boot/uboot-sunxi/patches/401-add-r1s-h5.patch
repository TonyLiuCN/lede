+CONFIG_DEFAULT_DEVICE_TREE="sun50i-h5-nanopi-r1s"
+CONFIG_SUN8I_EMAC=y
+CONFIG_USB_EHCI_HCD=y
+CONFIG_SYS_USB_EVENT_POLL_VIA_INT_QUEUE=y
